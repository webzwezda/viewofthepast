---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  
  {% for post in site.posts %}{% unless post.published == false %}
  <url>
    <loc>
      {{ post.url | remove: "index.html" | prepend: site.baseurl | prepend: site.url}}
    </loc>
  {% if post.dateModified %}
    <lastmod>
      {{ post.dateModified | date_to_xmlschema }}
    </lastmod>
    {% elsif post.date %}
    <lastmod>
      {{ post.date | date_to_xmlschema }}
    </lastmod>
    {% else %}
    <lastmod>
      {{ site.time | date_to_xmlschema }}
    </lastmod>
    {% endif %}
  </url>{% endunless %}
  {% endfor %}

</urlset>


